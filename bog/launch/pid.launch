<launch>

  <node ns = 'one' name="Front_left" pkg="pid" type="controller" output="screen" >
      <param name="Kp" value="3" />
      <param name="Ki" value="1" />
      <param name="Kd" value="0" />
      <param name="upper_limit" value="120" />
      <param name="lower_limit" value="-120" />
      <param name="windup_limit" value="50" />
      <param name="cutoff_frequency" value="5" />
      <param name="max_loop_frequency" value="105.0" />
      <param name="min_loop_frequency" value="95.0" />
     </node>


  <node ns = 'two' name="Front_right" pkg="pid" type="controller" output="screen" >
      <param name="Kp" value="3" />
      <param name="Ki" value="1" />
      <param name="Kd" value="0" />
      <param name="upper_limit" value="120" />
      <param name="lower_limit" value="-120" />
      <param name="windup_limit" value="50" />
      <param name="cutoff_frequency" value="5" />
      <param name="max_loop_frequency" value="105.0" />
      <param name="min_loop_frequency" value="95.0" />
     </node>



  <node ns = 'three' name="Back_right" pkg="pid" type="controller" output="screen" >
      <param name="Kp" value="3" />
      <param name="Ki" value="1" />
      <param name="Kd" value="0" />
      <param name="upper_limit" value="120" />
      <param name="lower_limit" value="-120" />
      <param name="windup_limit" value="50" />
      <param name="cutoff_frequency" value="5" />
      <param name="max_loop_frequency" value="105.0" />
      <param name="min_loop_frequency" value="95.0" />
     </node>

  <node ns = 'four' name="Back_left" pkg="pid" type="controller" output="screen" >
      <param name="Kp" value="3" />
      <param name="Ki" value="1" />
      <param name="Kd" value="0" />
      <param name="upper_limit" value="120" />
      <param name="lower_limit" value="-120" />
      <param name="windup_limit" value="50" />
      <param name="cutoff_frequency" value="5" />
      <param name="max_loop_frequency" value="105.0" />
      <param name="min_loop_frequency" value="95.0" />
     </node>
  
<node  ns = 'Port_1' pkg="rosserial_python" type="serial_node.py" name="serial_node" args="/dev/ttyACM1"/>
<node  ns = 'Port_0' pkg="rosserial_python" type="serial_node.py" name="serial_node" args="/dev/ttyACM0"/>




 <node pkg="bog" type="dist_to_pid.py" name="flag" />
<node pkg="bog" type="Wheel_linear_velocity.py" name="flagger" />
<node pkg="bog" type="imu.py" name="turning" />

<include file="$(find bog)/launch/MUX.launch"/>






</launch>
